{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      true,
      "always"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "trailing-comma": [
      false
    ],
    "no-console": [
      false
    ],
    "interface-name": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "variable-name": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "no-empty": [
      false
    ],
    "no-unused-expression": [
      false
    ],
    "no-var-requires": [
      false
    ]
  }
}
